@{
   // Layout = null;
 }
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="">
    
</head>
<body>

    <div id="wrapper"  ng-controller="homeController" style="position:relative;z-index:-2">
       
        <section class="homepage-slider" id="home-slider">
            <div class="flexslider">
                <ul class="slides">
                    <li>
                        <img src="~/Content/themes/images/carousel/banner-1.jpg" alt="" />
                        <div class="intro">
                            <h1>Aprovecha las ofertas</h1>
                            <p><span>10% de descuento</span></p>
                            <p><span>Sobre toda la mercancia on-line</span></p>
                        </div>
                    </li>
                    <li>
                        <img src="~/Content/themes/images/carousel/banner-2.jpg" alt="" />
                        <div class="intro">
                            <h1>Calidad inigualable</h1>
                            <p><span>Bolsas con el mejor Acabado</span></p>
                            <p><span>No esperes más...</span></p>
                        </div>
                    </li>
                </ul>
            </div>
        </section>
        <section class="header_text">
            Productos de la más fina calidad, elaborados con el mejor Cuero,
            <br />Directo desde el Perú.
        </section>
        <section class="main-content" style="position:relative;z-index:-1">
            <div class="row">
                <div class="span12">
                    <div class="row">
                        <div class="span12" style="position:relative;z-index:1">
                            <h4 class="title">
                                <span class="pull-left"><span class="text"><span class="line">Articulos en <strong>Oferta</strong></span></span></span>
                                <span class="pull-right">
                                    <a class="left button" href="#myCarousel" data-slide="prev"></a><a class="right button" href="#myCarousel" data-slide="next"></a>
                                </span>
                            </h4>
                            <div id="myCarousel" class="myCarousel carousel slide">
                                <div class="carousel-inner">
                                    <div class="active item">
                                        <ul class="thumbnails" >
                                            <li class="span3" ng-repeat="items in newproducts1">
                                                <div class="product-box">
                                                    <span class="sale_tag"></span>
                                                    <p><a href="/Productos/Details/{{items.id}}"><img src="~/Content/themes/images/ladies/{{items.foto}}" alt="" /></a></p>
                                                    <a href="/Productos/Details/{{items.id}}" class="title">{{items.nombre}}</a><br />
                                                    <a href="" class="category">{{items.category}}</a>
                                                    <p class="price">S/. {{items.precio}}</p>
                                                </div>
                                            </li>

                                        </ul>
                                    </div>
                                    <div class="item">
                                        <ul class="thumbnails"  >
                                            <li class="span3" ng-repeat="items in newproducts2">
                                                <div class="product-box">
                                                    <p><a href="/Productos/Details/{{items.id}}"><img src="~/Content/themes/images/ladies/{{items.foto}}" alt="" /></a></p>
                                                    <a href="/Productos/Details/{{items.id}}" class="title">{{items.nombre}}</a><br />
                                                    <a href="" class="category">{{items.category}}</a>
                                                    <p class="price">S/. {{items.precio}}</p>
                                                </div>
                                            </li>
                                           
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <div class="row">
                        <div class="span12">
                            <h4 class="title">
                                <span class="pull-left"><span class="text"><span class="line">Ultimos <strong>Productos</strong></span></span></span>
                                <span class="pull-right">
                                    <a class="left button" href="#myCarousel-2" data-slide="prev"></a><a class="right button" href="#myCarousel-2" data-slide="next"></a>
                                </span>
                            </h4>
                            <div id="myCarousel-2" class="myCarousel carousel slide">
                                <div class="carousel-inner">
                                    <div class="active item">
                                        <ul class="thumbnails">
                                            <li class="span3" ng-repeat="items in offerproducts1">
                                                <div class="product-box">
                                                    <span class="sale_tag"></span>
                                                    <p><a href="/Productos/Details/{{items.id}}"><img src="~/Content/themes/images/ladies/{{items.foto}}" alt="" /></a></p>
                                                    <a href="/Productos/Details/{{items.id}}" class="title">{{items.nombre}}</a><br />
                                                    <a href="" class="category">{{items.category}}</a>
                                                    <p class="price">S/. {{items.precio}}</p>
                                                </div>
                                            </li>
                                           </ul>
                                    </div>
                                    <div class="item">
                                        <ul class="thumbnails">
                                            <li class="span3" ng-repeat="items in offerproducts2">
                                                <div class="product-box">
                                                    <p><a href="/Productos/Details/{{items.id}}"><img src="~/Content/themes/images/ladies/{{items.foto}}" alt="" /></a></p>
                                                    <a href="/Productos/Details/{{items.id}}" class="title">{{items.nombre}}</a><br />
                                                    <a href="" class="category">{{items.category}}</a>
                                                    <p class="price">S/. {{items.precio}}</p>
                                                </div>
                                            </li>
                                           
                                        </ul>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row feature_box">
                        <div class="span4">
                            <div class="service">
                                <div class="responsive">
                                    <img src="~/Content/themes/images/feature_img_2.png" alt="" />
                                    <h4>DISEÑOS <strong>MODERNOS</strong></h4>
                                    <p></p>
                                </div>
                            </div>
                        </div>
                        <div class="span4">
                            <div class="service">
                                <div class="customize">
                                    <img src="~/Content/themes/images/feature_img_1.png" alt="" />
                                    <h4>ENVIO <strong>GRATIS A TODO EL PERU</strong></h4>
                                    <p></p>
                                </div>
                            </div>
                        </div>
                        <div class="span4">
                            <div class="service">
                                <div class="support">
                                    <img src="~/Content/themes/images/feature_img_3.png" alt="" />
                                    <h4>VENTAS <strong>24/7</strong></h4>
                                    <p></p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="our_client">
            <h4 class="title"><span class="text">Clientes</span></h4>
            <div class="row">
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/14.png"></a>
                </div>
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/35.png"></a>
                </div>
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/1.png"></a>
                </div>
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/2.png"></a>
                </div>
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/3.png"></a>
                </div>
                <div class="span2">
                    <a href="#"><img alt="" src="~/Content/themes/images/clients/4.png"></a>
                </div>
            </div>
        </section>
        
    </div>
   <script>
       var categorys = JSON.parse('@Html.Raw(ViewBag.categorys)');
       var newProd = JSON.parse('@Html.Raw(ViewBag.offerProd)');
       var offerProd = JSON.parse('@Html.Raw(ViewBag.NewProd)');
   </script>
</body>
</html>